{
    "name": "bfg/text",
    "description": "All sorts of chips to work with the text",
    "type": "bfg-app",
    "keywords": ["laravel", "extension", "text"],
    "license": "MIT",
    "authors": [
        {
            "name": "bfg"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "laravel/framework": "^8.0",
        "bfg/installer": "*",
        "bfg/object": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Text\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bfg\\Text\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Text\\ServiceProvider"
            ]
        }
    }
}
